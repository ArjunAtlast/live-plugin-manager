{
  "name": "live-plugin-manager",
  "version": "0.4.0",
  "description": "Install and uninstall any node package at runtime from npm registry",
  "keywords": [
    "plugin",
    "package",
    "npm",
    "install"
  ],
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "src-build": "tsc",
    "src-build-w": "tsc -w",
    "start-demo": "nodemon ./demo/index.js -e js",
    "develop": "npm run src-build-w",
    "test": "mocha --require source-map-support/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davideicardi/live-plugin-manager.git"
  },
  "author": "Davide Icardi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davideicardi/live-plugin-manager/issues"
  },
  "homepage": "https://github.com/davideicardi/live-plugin-manager#readme",
  "devDependencies": {
    "@types/chai": "4.0.4",
    "@types/mocha": "2.2.42",
    "@types/node": "8.0.24",
    "chai": "4.1.1",
    "mocha": "3.5.0",
    "nodemon": "1.11.0",
    "source-map-support": "0.4.16",
    "tslint": "5.6.0",
    "typescript": "2.4.2"
  },
  "dependencies": {
    "@types/debug": "0.0.30",
    "@types/fs-extra": "4.0.0",
    "@types/lockfile": "1.0.0",
    "@types/request": "2.0.2",
    "@types/semver": "5.4.0",
    "@types/url-join": "0.8.2",
    "debug": "3.0.0",
    "fs-extra": "4.0.1",
    "lockfile": "1.0.3",
    "npm-registry-client": "8.5.0",
    "npmlog": "4.1.2",
    "request": "2.81.0",
    "semver": "5.4.1",
    "tar.gz": "1.0.5",
    "url-join": "2.0.2"
  }
}
