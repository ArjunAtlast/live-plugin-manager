{
  "name": "live-plugin-manager",
  "version": "0.8.1",
  "description": "Install and uninstall any node package at runtime from npm registry",
  "keywords": [
    "plugin",
    "package",
    "npm",
    "install"
  ],
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "src-build": "tsc",
    "src-build-w": "tsc -w",
    "start-demo": "nodemon ./demo/index.js -e js",
    "develop": "npm run src-build-w",
    "test": "mocha --require source-map-support/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davideicardi/live-plugin-manager.git"
  },
  "author": "Davide Icardi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davideicardi/live-plugin-manager/issues"
  },
  "homepage": "https://github.com/davideicardi/live-plugin-manager#readme",
  "devDependencies": {
    "@types/chai": "4.0.4",
    "@types/mocha": "2.2.44",
    "@types/node": "8.0.47",
    "chai": "4.1.2",
    "mocha": "4.0.1",
    "nodemon": "1.12.0",
    "source-map-support": "0.5.0",
    "tslint": "5.8.0",
    "typescript": "2.6.1"
  },
  "dependencies": {
    "@types/debug": "0.0.30",
    "@types/fs-extra": "4.0.3",
    "@types/lockfile": "1.0.0",
    "@types/request": "2.0.7",
    "@types/semver": "5.4.0",
    "@types/url-join": "0.8.2",
    "@types/tar": "4.0.0",
    "debug": "3.1.0",
    "fs-extra": "4.0.2",
    "github": "12.0.1",
    "lockfile": "1.0.3",
    "npm-registry-client": "8.5.0",
    "npmlog": "4.1.2",
    "request": "2.83.0",
    "semver": "5.4.1",
    "tar": "4.0.2",
    "url-join": "2.0.2"
  }
}
